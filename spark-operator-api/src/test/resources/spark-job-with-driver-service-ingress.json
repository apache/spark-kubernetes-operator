{
  "runtimeVersions": {
    "sparkVersion": "test"
  },
  "driverServiceIngressList": [
    {
      "serviceMetadata": {
        "name": "spark-ui-service"
      },
      "serviceSpec": {
        "ports": [
          {
            "protocol": "TCP",
            "port": 80,
            "targetPort": 4040
          }
        ]
      },
      "ingressMetadata": {
        "name": "spark-ui-ingress",
        "annotations": {
          "nginx.ingress.kubernetes.io/rewrite-target": "/"
        }
      },
      "ingressSpec": {
        "ingressClassName": "nginx-example",
        "rules": [
          {
            "http": {
              "paths": [
                {
                  "path": "/",
                  "pathType": "Prefix",
                  "backend": {
                    "service": {
                      "name": "spark-ui-service",
                      "port": {
                        "number": 80
                      }
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
